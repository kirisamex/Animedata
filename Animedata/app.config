<?xml version="1.0"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <appSettings>
    <!--SqlServer连接参数-->
    
    <!--数据库地址-->
    <add key="DBServerIP" value="localhost" />
    
    <!--数据库名称-->
    <add key="DBName" value="ANIMEDATA" />
    
    <!--数据库帐号-->
    <add key="DBAccount" value="" />
    
    <!--数据库密码-->
    <add key="DBPassword" value="" />
    
    <!--Windows认证连接：True/False-->
    <add key="DBIS" value="true"/>
  </appSettings>
</configuration>
